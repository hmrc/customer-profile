<endpoint>
  <name>Get Preferences</name>
  <description>This endpoint retrieves the preferences.</description>
  <section id="resource">
    <title>Resource</title>
    <resource>/profile/preferences </resource>
  </section>
  <section id="path-parameters">
    <title>Path Parameters</title>
    <table>
      <headings>
        <cell>Name</cell>
        <cell>Type</cell>
        <cell>Example</cell>
        <cell>Description</cell>
      </headings>
      <rows>
        <row>
          <cell><code></code></cell>
          <cell><code></code></cell>
          <cell><code></code></cell>
          <cell></cell>
        </row>
      </rows>
    </table>
  </section>
  <section id="authorisation">
    <title>Authorisation</title>
    <authorisation>
      <type>USER</type>
      <scope>read:customer-profile</scope>
    </authorisation>
  </section>
  <section id="content-types">
    <title>Accepted Media Types</title>
    <list>
      <item>
        <code>application/vnd.hmrc.1.0+json</code>
      </item>
    </list>
  </section>
  <section id="sample-response">
    <title>Response</title>
    <httpStatus>200 (OK)</httpStatus>
    <json><![CDATA[{
    "digital" : true,
    "email" : {
      "email" : "name@email.co.uk",
      "status" : "verified"
    }
}]]></json>
    <table>
      <headings>
        <cell>Name</cell>
        <cell>Type</cell>
        <cell>Example</cell>
        <cell>Description</cell>
      </headings>
      <rows>
        <row>
          <cell><code>digital</code></cell>
          <cell><code>Boolean</code></cell>
          <cell><code>true</code></cell>
          <cell>The user's chosen preference for paperless settings</cell>
        </row>
        <row>
          <cell><code>email.email</code></cell>
          <cell><code>Email Address</code></cell>
          <cell><code>name@email.co.uk</code></cell>
          <cell>A valid email address for the user</cell>
        </row>
        <row>
          <cell><code>email.status</code></cell>
          <cell><code>String</code></cell>
          <cell><code>verified</code></cell>
          <cell>The verification status of the user's email address</cell>
        </row>
      </rows>
    </table>
  </section>
  <section id="error-responses">
    <title>Error Scenarios</title>
    <table>
      <headings>
        <cell>Error Scenario</cell>
        <cell>HTTP Status</cell>
        <cell>Code</cell>
      </headings>
      <rows>
        <row>
          <cell>Invalid NINO</cell>
          <cell><code>400 (Bad Request)</code></cell>
          <cell><code>NINO_INVALID</code></cell>
        </row>
      </rows>
    </table>
  </section>
</endpoint>