{
  "scopes":[
    {
      "key":"read:customer-profile",
      "name":"Execute customer-profile call",
      "description":"Retrieve the customer profile"
    }
  ],
  "api":{
    "name":"customer-profile",
    "description":"The API is used to retrieve customer profile information.",
    "context":"customer-profile",
    "requiresTrust": true,
    "versions":[
      {
        "version":"1.0",
        "status":"PROTOTYPED",
        "endpoints":[
          {
            "uriPattern":"/profile/accounts",
            "endpointName":"Get Accounts",
            "method":"GET",
            "authType":"USER",
            "throttlingTier":"UNLIMITED",
            "scope":"read:customer-profile"
          },
          {
            "uriPattern":"/profile/personal-details/:nino",
            "endpointName":"Get Personal Details",
            "method":"GET",
            "authType":"USER",
            "throttlingTier":"UNLIMITED",
            "scope":"read:customer-profile"
          },
          {
            "uriPattern":"/profile/preferences",
            "endpointName":"Get Preferences",
            "method":"GET",
            "authType":"USER",
            "throttlingTier":"UNLIMITED",
            "scope":"read:customer-profile"
          },
          {
            "uriPattern":"/profile/preferences/paperless-settings ",
            "endpointName":"Set Paperless Preferences",
            "method":"POST",
            "authType":"USER",
            "throttlingTier":"UNLIMITED",
            "scope":"read:customer-profile"
          }
        ]
      }
    ]
  }
}
